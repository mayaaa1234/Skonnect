    width: 100%;
    border-radius: 5px;
    padding: 6px;
  }

  .sidebar-items {
    width: 100%;
    margin-bottom: 4px;
  }

  .hide-sidebar-wrapper {
    .sidebar-btn-hide-sidebar {
      margin-bottom: 18px;

      .sidebar-shrink-icon {
        transform: rotate(180deg);
      }
    }
  }
}

