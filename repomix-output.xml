      height: 67px !important;
    }
  }
}

@include xl {
  nav {
    // changed to grid cause nav-links is at bottom of the html structure
    // so that it can be inserted as second elem in nav
    display: grid;
    grid-template-columns: auto 1fr auto auto;
    grid-template-areas: "logo links theme action";
    align-content: center;

    // hide mobile-only elements on xl screens
    .nav-menu-btn-open,
    .nav-menu-popup {
      display: none;
    }

    .logo-container {
      grid-area: logo;
    }

    .nav-links {
      grid-area: links;
      justify-self: center;
      display: flex;
      gap: 34px;
    }

    .theme-btn {
      grid-area: theme;
    }

    // both nav-auth continer and profile
    // has grid area action to put them in that grid cell
    // whichever is available based on session status
    .nav-auth-container {
      grid-area: action;
      display: flex;
      align-items: center;
      gap: 1rem;
    }

    .profile {
      grid-area: action;
      display: block !important;
    }

    .signup-link,
    .login-link {
      text-align: center;
      display: block;
      width: 100px;
    }

    a {
      text-decoration: none;
    }
  }
}
