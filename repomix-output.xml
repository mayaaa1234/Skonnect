@use "../abstracts/mixins" as *;

body[data-page="about"] {
  &[data-theme="dark"] {
    .container {
      box-shadow: 0px 0px 10px rgba(255, 255, 255, 0.1);
    }
  }

  &[data-theme="light"] {
    .container {
      box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
    }
