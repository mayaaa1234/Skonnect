$font-size: (
  "n2": 0.6rem,
  "n1": 0.8rem,
  "1": 1rem,
  "2": 1.5rem,
  "3": 2rem,
  "4": 2.5rem,
  "5": 3rem,
  "6": 3.5rem,
  "7": 4rem,
  "8": 4.5rem,
  "9": 5rem,
);

@each $size, $value in $font-size {
  @include xs {
    .fs-#{$size}-xs {
      font-size: $value;
    }
  }
  @include sm {
    .fs-#{$size}-sm {
      font-size: $value;
    }
  }
  @include md {
    .fs-#{$size}-md {
      font-size: $value;
    }
  }
  @include lg {
    .fs-#{$size}-lg {
      font-size: $value;
    }
  }
  @include xl {
    .fs-#{$size}-xl {
      font-size: $value;
    }
  }
}
