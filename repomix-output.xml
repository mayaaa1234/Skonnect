<file path="frontend/src/scss/pages/_home.scss">
@use "../abstracts/mixins" as *;
@use "../abstracts/variables" as *;
@use "../layout/nav" as *;

$lg-margin-top: 24px;

body[data-page="home"] {
  display: flex;
  flex-direction: column;

  %titles {
    padding-bottom: 5px;
    font-weight: bold;
  }

  .services-txt {
    @extend %titles;
    @include xs {
      font-size: $font-size-md;
    }

    @include xl {
      padding-left: 8px;
      font-size: $font-size-lg;
      text-align: center;
    }
  }

  // ------------------------------
  // for scroll animation
  .hidden {
    opacity: 0;
    transform: translateY(20px);
    transition:
      opacity 0.6s ease-out,
      transform 0.6s ease-out;
  }
  .show {
    opacity: 1;
    transform: translateY(0);
  }
  // ------------------------------

  .banner-top {
    display: grid;
    grid-template: repeat(2, auto) / repeat(2, auto);
    padding: 1rem;
    align-items: center;
    gap: 5px;
    background-color: var(--neutral);

    .sk-logo-1 {
      grid: 1 / 1;
      // border: 1px solid white;
    }

    .sk-logo-2 {
      justify-self: end;
    }

    .banner-info {
      margin-top: $base-padding * -5;
      grid-row: 2;
      grid-column: 1 /3;

      p {
        font-size: 14px !important;
      }
    }

    .banner-info p {
      font-size: 12px;
    }

    @include md {
      grid-template-columns: repeat(3, auto);

      .sk-logo-2,
      .sk-logo-1 {
        justify-self: center;
      }

      .banner-info,
      .sk-logo-2,
      .sk-logo-1 {
        grid-row: initial;
        grid-column: initial;
      }

      .banner-info {
        margin-top: 0 !important;
      }
    }

    @include lg {
      .img-l {
        width: 96px;
      }

      .img-r {
        width: 90px;
      }
    }

    @include xl {
      .img-l {
        width: 113px;
      }

      .img-r {
        width: 102px;
      }
    }
  }

  // Hero

  section.hero {
    .container {
      flex-direction: column;
      max-width: 1400px;
    }

    .hero-text-container {
      h1 {
        line-height: 1.2;
      }
    }

    .img-container {
      position: relative;
      // border: 1px solid white;

      &::before {
        content: "";
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 300px;
        height: 300px;
        z-index: -1;
        background-color: var(--accent);
        border-radius: 50%;
        background: linear-gradient(
          to right bottom,
          rgb(0, 193, 174),
          rgb(0, 165, 191),
          rgb(0, 108, 166)
        );
        // opacity: 0.8;
        filter: blur(100px);
      }
    }

    @include lg {
      margin-top: $lg-margin-top;

      .hero-container {
        flex-direction: row;
        justify-content: space-evenly;
      }

      $sk-char-width: 220px;

      .img-container {
        min-height: $sk-char-width;
        // margin-right: 100px;
        // margin: 0 auto;

        img.sk-char {
          width: $sk-char-width !important;
        }
      }
    }
  }

  // Services

  section.services {
    flex: 1;

    @include lg {
      margin-top: $lg-margin-top;
    }

    h3 {
      // border-bottom: 1 solid rgba(var(--accent), 0.1);

      @include lg {
        text-align: center;
      }
    }

    .cards-container {
      margin-inline: auto;
      max-width: 1400px;
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
      // justify-items: center;

      @include max-bp(660px) {
        p {
          justify-self: center !important;
        }
      }

      @include xl {
        margin-top: 50px;
      }

      p {
        margin-bottom: -30px;
        justify-self: start;
        grid-column: 1 / -1;
      }

      .card {
        // margin-inline: auto;
        background-color: var(--secondary);
        cursor: pointer;
        perspective: 1000px;
        transition:
          transform 0.3s ease,
          box-shadow 0.3s ease;

        @include max-bp(660px) {
          width: min(100%, 450px);
          margin: 0 auto;
        }

        @include md {
          // max-width: unset;
        }

        &:hover {
          transform: scale(1.05) rotateY(10deg) rotateX(10deg);
          box-shadow: 0 20px 30px rgba(0, 0, 0, 0.1);
        }
        & > div:first-child {
          display: flex;
          align-items: center;
          justify-content: center;
          align-items: center;
          gap: 10px;
          border-left: 4px solid var(--accent);
          height: 60px;
          // border: 1px solid white;

          @include lg {
            height: 80px;
          }
        }
      }
    }
  }

  // ------------------------------------------------------------------------ //

  // Carousel

  .infinite-carousel {
    position: relative;
    overflow: hidden;
    width: min(100%, 2300px);
    margin: 0 auto;
    /* hide scrollbar if any */
    scrollbar-width: none; /* Firefox */
    -ms-overflow-style: none; /* IE 10+ */
  }
  .infinite-carousel::-webkit-scrollbar {
    display: none;
  }

  .carousel-track {
    display: flex;
    // gap: 10px;
    /* no animation here */

    @include max-bp("475px") {
      gap: 40px;
    }
  }

  .carousel-item {
    flex: 0 0 calc(100% / 3); /* 3 items visible at once */

    img {
      border-radius: 5px;
      min-width: 300px;
      max-width: 500px;
      height: 500px;
    }
  }
}
</file>

