                    <img
                        src="/sk-data/sk-info/8.png"
                        class="h-350px"
                        style="width: 100%"
                    />
                </div>

                <a class="prev" data-slide-control="prev">&#10094;</a>
                <a class="next" data-slide-control="next">&#10095;</a>

                <div style="text-align: center">
                    <span class="dot dot-sm" data-dot="1"></span>
                    <span class="dot dot-sm" data-dot="2"></span>
                    <span class="dot dot-sm" data-dot="3"></span>
                    <span class="dot dot-sm" data-dot="4"></span>
