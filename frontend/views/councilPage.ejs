                <div class="slides-sm fade">
                    <img
                        src="/sk-data/sk-info/1.png"
                        class="h-350px"
                        style="width: 100%"
                    />
                </div>
                <div class="slides-sm fade">
                    <img
                        src="/sk-data/sk-info/2.png"
                        class="h-350px"
                        style="width: 100%"
                    />
                </div>
                <div class="slides-sm fade">
                    <img
                        src="/sk-data/sk-info/3.png"
                        class="h-350px"
                        style="width: 100%"
                    />
                </div>
                <div class="slides-sm fade">
