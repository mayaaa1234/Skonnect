export default async function initBudgetAllocationPage() {}

interface BudgetAllocation {
  category: string;
  amount: number;
  items: string;
}

async function getAllBudgetAllocation(): Promise<BudgetAllocation[]> {
  const res = await fetch("/api/v1/budgetAllocation/", {
    method: "GET",
    credentials: "include",
  });

  if (!res.ok) {
    throw new Error(`HTTP error! status: ${res.status}`);
  }

  return await res.json();
}

async function updateBudgetAllocation(): Promise<void> {
  const res = await fetch("/api/v1/budgetAllocation/", {
    method: "UPDATE",
    credentials: "include",
  });

  if (!res.ok) {
    throw new Error(`HTTP error! status: ${res.status}`);
  }

  return await res.json();
}
